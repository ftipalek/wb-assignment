{
	"name": "ftipalek/wb-assignment",
	"description": "WB assignment",
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.3",
		"dibi/dibi": "^5.1",
		"latte/latte": "^3.0",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"tracy/tracy": "^2.10"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-deprecation-rules": "^2.0",
		"phpstan/phpstan-dibi": "^2.0",
		"phpstan/phpstan-nette": "^2",
		"phpstan/phpstan-strict-rules": "^2.0",
		"slevomat/coding-standard": "^8.20",
		"spaze/phpstan-disallowed-calls": "^4.6",
		"squizlabs/php_codesniffer": "^3.13",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"WbAssignment\\": "app"
		}
	},
	"scripts": {
		"codesniffer": "vendor/bin/phpcs --standard=../phpcs.xml --extensions=php,phpt --colors -p -s ./app",
		"codesnifferFix": "vendor/bin/phpcbf --standard=../phpcs.xml --extensions=php,phpt --colors -p -s ./app",
		"phpstan": "phpstan analyse -c ../phpstan.neon",
		"server": "cd ./www && php -S localhost:8005",
		"tester": "tester tests -s"
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"symfony/thanks": true
		},
		"platform": {
			"php": "8.3"
		},
		"sort-packages": true
	}
}
